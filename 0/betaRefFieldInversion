    b4-inlet
    {
        type            zeroGradient;

    }
    b5-outlet
    {
        type            zeroGradient;
    }
    b6-humpWall
    {
        type            zeroGradient;

    }
    b7-topWall
    {
        type            zeroGradient;

    }
    b2-symmetry1
    {
        type            symmetry;

    }
    b3-symmetry2
    {
        type            symmetry;

    }
}


