FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];


internalField   uniform 1.0684e-08;

boundaryField
{
    b2-symmetry1
    {
        type            symmetry;
    }
    b3-symmetry2
    {
        type            symmetry;
    }
    b4-inlet
    {
        type            calculated;
        value           uniform 1.0684e-08;
    }
    b5-outlet
    {
        type            zeroGradient;
    }
    b6-humpWall
    {
        type            nutUSpaldingWallFunction;
        value           uniform 1e-16;
    }
    b7-topWall
    {
        type            nutUSpaldingWallFunction;
        value           uniform 1e-16;
    }
    procBoundary2to3
    {
        type            processor;
        value           uniform 1.0684e-08;
    }
}


