FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nuTilda;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];


internalField   uniform 0.00011096826;

boundaryField
{
    b2-symmetry1
    {
        type            symmetry;
    }
    b3-symmetry2
    {
        type            symmetry;
    }
    b4-inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    b5-outlet
    {
        type            inletOutlet;
        inletValue      nonuniform 0();
        value           nonuniform 0();
    }
    b6-humpWall
    {
        type            fixedValue;
        value           uniform 0;
    }
    b7-topWall
    {
        type            fixedValue;
        value           uniform 0;
    }
    procBoundary1to0
    {
        type            processor;
        value           uniform 0.00011096826;
    }
    procBoundary1to3
    {
        type            processor;
        value           uniform 0.00011096826;
    }
}


